{
  "engines": {
    "node": ">=4.0.0"
  },
  "config": {
    "MOCHA_OPTS": "test/**/*.spec.* --compilers .:babel-core/register --colors --recursive --require ./test/setup.js"
  },
  "scripts": {
    "start": "gulp serve",
    "test": "mocha $npm_package_config_MOCHA_OPTS",
    "test:watch": "npm test -- --reporter Landing --watch --watch-extensions jsx",
    "build": "gulp clean && gulp pug && gulp styles && gulp js:prod && gulp build && gulp serve:dist",
    "prod": "gulp clean && gulp pug && gulp styles && gulp js:prod && gulp build",
    "cover": "rm -rf coverage && babel-node ./node_modules/istanbul/lib/cli cover _mocha -- $npm_package_config_MOCHA_OPTS",
    "cover:all": "rm -rf coverage && babel-node ./node_modules/istanbul/lib/cli --include-all-sources cover _mocha -- $npm_package_config_MOCHA_OPTS && open coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.3.13",
    "browser-sync": "^2.11.2",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "doiuse": "^2.3.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.0",
    "gulp-debug": "^2.1.2",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-minify-html": "^1.0.6",
    "gulp-newer": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.1.0",
    "gulp-pug": "^2.0.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.0.8",
    "gulp-util": "^3.0.7",
    "gulp.spritesmith": "^6.2.1",
    "main-bower-files": "^2.11.1",
    "mocha": "^2.4.5",
    "opn": "^4.0.1",
    "postcss-assets": "^4.0.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-easy-import": "^1.0.1",
    "postcss-import": "^8.1.0",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-short": "^1.4.0",
    "postcss-sprites": "^3.1.2",
    "stylelint": "^5.2.1",
    "stylelint-config-standard": "^4.0.1",
    "through2": "^2.0.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-notifier": "^1.3.0",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
    "gsap": "^1.18.4",
    "lodash": "^4.8.2",
    "moment": "^2.13.0",
    "npm-modernizr": "^2.8.3",
    "rx": "^4.1.0",
    "rx-lite": "^4.0.8",
    "sanitize.css": "^3.3.0"
  }
}
